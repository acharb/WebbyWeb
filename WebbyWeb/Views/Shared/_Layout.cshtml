<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>WebbyWeb</title>

    <!--     Fonts and icons     -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" />
    <link href="https://fonts.googleapis.com/css?family=Barlow:300,400" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />

    <!-- CSS Files -->
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap/dist/css/bootstrap.min.css"  />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/lib/bootstrap/dist/css/material-kit.css")"  />

    <!--   Core JS Files   -->
    <script src="~/lib/jquery/dist/jquery.min.js" type="text/javascript"></script>
	<script src="~/lib/jquery/dist/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/lib/bootstrap/dist/js/material.min.js")" type="text/javascript"></script>

    <!-- Jquery Files -->
    <script src="@Url.Content("~/lib/jquery-validation/dist/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/lib/jquery-validation/dist/additional-methods.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    <!-- 1-day Calendar Files -->
    <link rel="stylesheet" type="text/css" href="~/css/own-calendar.css"/>
    <!-- <script src="~/js/calendar/cv-calendar-scheduler.js"></script>
	<script src="~/js/calendar/cv-calendar-time.js"></script>
	<script src="~/js/calendar/cv-calendar-template.js"></script> -->

    <!-- chart from Material Design Bootstrap -->
    <script src="~/js/chart/mdb.min.js"></script>
    <script src="~/js/chart/popper.min.js"></script>

    <!--  Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
    <script src="~/lib/bootstrap/dist/js/nouislider.min.js" type="text/javascript"></script>

    <!--  Plugin for the Datepicker, full documentation here: http://www.eyecon.ro/bootstrap-datepicker/ -->
    <script src="~/lib/bootstrap/dist/js/bootstrap-datepicker.js" type="text/javascript"></script>

    <!-- Control Center for Material Kit: activating the ripples, parallax effects, scripts from the example pages etc -->
	<script src="@Url.Content("~/lib/bootstrap/dist/js/material-kit.js")" type="text/javascript"></script>



    <!-- Need to Add for Modal in order to close (fixes Chrome bug)-->
    <style>
        body.modal-open div.modal-backdrop { 
            z-index: 0; 
        }

        body{
            background:white;
            font-family: 'Barlow', sans-serif;
        }
        

    </style>

<!-- ***using creative Time bootstrap framework:
 https://www.creative-tim.com/product/material-kit?ref=presentation-page -->



</head>
<body>
		
<!-- NavBar Start-->		
<nav class="navbar navbar-default" role="navigation" style="text-transform:lowercase; color:gray; background-color:white">  
    <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
        </div>

        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav" >
                <li><a style="text-transform:lowercase;" asp-area="" asp-controller="Home" asp-action="StartHabits">start</a></li>
                <li><a style="text-transform:lowercase;" asp-area="" asp-controller="Home" asp-action="Habits">daily</a></li>
                <li><a style="text-transform:lowercase;" asp-area="" asp-controller="Home" asp-action="Progress">progress</a></li>
            </ul>
        </div>
    </div>
</nav>
<!-- NavBar End-->    
		
<div class="wrapper" style="text-transform: lowercase">
    
    <!-- note: changed .header-filter:after of material-kit.css to get rid of background fade look -->
    <!-- <div class="header header-filter" style="min-height:350px; background-size:contain; background-image: url('/images/ladder_clouds.jpg');"></div>  -->
    <div class="header header-filter" style="min-height: 70px"></div>
    <!-- style="background-repeat: no-repeat; background-attachment: fixed;background-position:bottom; background-size:contain; margin: -60px 100px 0px; background-image: url('/images/pyramid_BandW.jpg');" -->


    <!-- you can use the class main-raised if you want the main area to be as a page with shadows -->
    <div class="main main-raised" id='mainCard' style="background:white; margin: -60px 200px 0px;"> <!-- margin controls card size -->
        <div class="container" style="width:inherit">

            <!-- here you can add your content -->
            

					
            @RenderBody()
        
        <footer>
           
        </footer>
        </div>
    </div>
</div>

</body>

</html>

